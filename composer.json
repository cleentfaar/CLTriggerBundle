{
  "name": "cleentfaar/trigger-bundle",
  "description": "A Symfony bundle that let's you execute handlers on incoming GET parameters",
  "keywords": ["trigger", "get", "handle", "redirect"],
  "type": "symfony-bundle",
  "license": "MIT",
  "authors": [
    {
      "name": "Cas Leentfaar",
      "homepage": "https://github.com/cleentfaar",
      "email": "info@casleentfaar.com"
    }
  ],
  "require": {
    "php": ">=5.4",
    "symfony/framework-bundle": "~2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.2",
    "matthiasnoback/symfony-config-test": "~0.2",
    "matthiasnoback/symfony-dependency-injection-test": "~0.7"
  },
  "autoload": {
    "psr-4": {
      "CL\\Bundle\\TriggerBundle\\": ""
    }
  },
  "config": {
    "bin-dir": "bin"
  },
  "target-dir": "CL/Bundle/TriggerBundle"
}
